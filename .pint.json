{
  "preset": "laravel",
  "exclude": [
    "database/migrations",
    "vendor",
    "storage"
  ],
  "rules": {
    "ordered_imports": {"sort_algorithm": "alpha"},
    "no_unused_imports": true,
    "trailing_comma_in_multiline": {"after_heredoc": true, "elements": ["arguments", "arrays", "parameters"]}
  }
}